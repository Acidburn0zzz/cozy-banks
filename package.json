    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-env": "^1.0.2",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
