{
  "name": "cozy-bank",
  "version": "0.0.2",
  "main": "src/main.jsx",
  "scripts": {
    "build": "npm run build:browser",
    "build:browser": "NODE_ENV=browser:production npm run commons:build",
    "build:mobile": "NODE_ENV=mobile:production npm run commons:build -- --env.target=mobile",
    "clean:browser": "rm -rf build/*",
    "clean:mobile": "rm -rf src/targets/mobile/www",
    "commons:build": "webpack --config webpack.config.js --bail",
    "commons:watch": "webpack --config webpack.config.js --watch --progress",
    "commons:watch:standalone": "webpack-dev-server --config webpack.config.js --port 8282 --inline --hot",
    "get-deploy-branch": "test $(git rev-parse --abbrev-ref HEAD) = 'prod' && echo latest || echo build",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-$(npm run get-deploy-branch -s)} --repo git@gitlab.cozycloud.cc:labs/cozy-bank.git",
    "tx": "true ||Â tx pull --all || true",
    "precommons:build": "npm-run-all lint tx",
    "prebuild:browser": "npm run clean:browser",
    "prebuild:mobile": "npm run clean:mobile",
    "pretest": "npm run lint",
    "prewatch:browser": "npm run clean:browser",
    "prewatch:mobile": "npm run clean:mobile",
    "lint": "npm-run-all --parallel 'lint:*'",
    "lint:js": "eslint --parser babel-eslint \"'src/**/*.js' 'src/**/*.jsx' 'test/**/*.js'\"",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "stack:docker": "docker run --rm -it -p 8080:8080 -p 8025:8025 -v \"$(pwd)/build/\":/data/cozy-app/bank cozy/cozy-app-dev",
    "test": "jest",
    "watch": "npm run watch:browser",
    "watch:browser": "NODE_ENV=browser:development npm run commons:watch",
    "watch:mobile": "NODE_ENV=mobile:development npm run commons:watch -- --env.target=mobile",
    "watch:browser:standalone": "NODE_ENV=browser:development npm run commons:watch:standalone",
    "watch:mobile:standalone": "NODE_ENV=mobile:development npm run commons:watch:standalone",
    "mobile:icon": "(cd src/targets/mobile && splashicon-generator --imagespath='./res/model')",
    "mobile:prepare": "(cd src/targets/mobile && cordova prepare)",
    "android:run": "(cd src/targets/mobile && cordova run android --device)",
    "android:run:emulator": "(cd src/targets/mobile && cordova run android --emulator)",
    "android:release": "npm run build:mobile && (cd src/targets/mobile && cordova build android --release)",
    "android:signed": "npm run android:release && (cd src/targets/mobile && apksigner sign --ks keys/android/<APP_NAME>-release-key.jks --out build/android/<APP_NAME>.apk platforms/android/build/outputs/apk/android-release-unsigned.apk)",
    "android:publish": "npm run android:signed && (cd src/targets/mobile && fastlane supply)",
    "ios:run": "(cd src/targets/mobile && cordova run ios --device)",
    "ios:run:emulator": "(cd src/targets/mobile && cordova run ios --emulator)",
    "ios:publish": "npm run build:mobile && (cd src/targets/mobile && fastlane ios testflight)",
    "commons:services": "webpack --config config/webpack.target.services.js",
    "build:services": "NODE_ENV=services:production yarn run commons:services",
    "watch:services": "NODE_ENV=services:development yarn run commons:services --watch",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "styleguide:deploy": "yarn git-directory-deploy -- --directory styleguide --branch master --repo docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/<USERNAME_GH>/<SLUG_GH>.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/<USERNAME_GH>/<SLUG_GH>/issues"
  },
  "homepage": "https://github.com/<USERNAME_GH>/<SLUG_GH>#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
